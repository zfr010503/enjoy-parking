page{
    height: 100%;
}

.top_view{
    position: fixed;
    top: 50rpx;
    left: 28rpx;
    right: 28rpx;
    height: 225rpx;
    background: white;
    box-shadow: 1rpx 1rpx 15rpx 15rpx rgba(165, 165, 165, 0.10);
    overflow: hidden;
    border-radius: 18rpx;
}

.bottom_view{
    position: fixed;
    bottom: 0px;
    width: 100%;
    height: 225rpx;
    background: white;
    box-shadow: 1rpx 1rpx 15rpx 15rpx rgba(165, 165, 165, 0.10);
    overflow: hidden;
    border-radius: 20rpx;
}