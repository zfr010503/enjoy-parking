/* pages/map_page/map_page.wxss */
page{
    height: 100%;
}

map{
    height: 100%;
    width: 100%;
}

#outsearch{
    position:absolute;
    bottom: 0px;
}
#ininput{
    box-shadow:1rpx 5rpx 10rpx 10rpx #00000006;
}
.search_view{
    position: absolute;
    width: 100%;
    height: 120rpx;
    border-radius: 30rpx;
}
.search_icon{
    position: absolute;
    top: 42rpx;
    left: 35rpx;
    height: 40rpx;
    width: 40rpx;
}
.input{
    position: absolute;
    top: 10rpx;
    left: 15rpx;
    right: 15rpx;
    padding-left: 70rpx;
    height: 100rpx;
    border-radius: 30rpx;
    background: rgb(255, 255, 255);
    box-shadow:1rpx 1rpx 10rpx 10rpx #00000008;
}

scroll-view{
    position: absolute;
    top: 137rpx;
    width: 100%;
}

.suggest{
    height: 135rpx;
    width: 100%;
    border-bottom: 3rpx solid #c9c9c9b7;
}

.icon{
    position: absolute;
    left: 28rpx;
    width: 35rpx;
    height: 45rpx;
}



.findme {
    position: fixed;
    left: 2%;
    bottom: 300rpx;
    width: 80rpx;
    height: 80rpx;
}